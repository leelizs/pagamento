{
    "version": 2,
    "builds": [
      {
        "src": "api/create-qrcode.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/verificar-status.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/create-qrcode",
        "dest": "/api/create-qrcode.js"
      },
      {
        "src": "/api/verificar-status",
        "dest": "/api/verificar-status.js"
      }
    ]
  }
  